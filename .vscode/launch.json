    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Python 调试程序: 当前文件",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "SGLang 启动服务",
            "type": "python",
            "request": "launch",
            "module": "sglang.launch_server",
            "args": [
                "--model-path",
                "/data/jieni/workspace/models/Qwen2.5-0.5B-Instruct/"
            ],
            "console": "integratedTerminal",
            "stopOnEntry": true,
            "justMyCode": false
        },
        {
            "name": "离线批量推理",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/examples/runtime/engine/offline_batch_inference.py",
            "args": [
                "--model",
                "/data/jieni/workspace/models/Qwen2.5-0.5B-Instruct/"
            ],
            "console": "integratedTerminal",
            "stopOnEntry": true,
            "justMyCode": false
        },
        {
            "name": "多模态离线批量推理",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/examples/runtime/engine/offline_batch_inference_vlm.py",
            "args": [
                "--model",
                "/data/jieni/workspace/models/Qwen2.5-VL-3B-Instruct/",
                "--chat-template=qwen25-vl",
            ],
            "console": "integratedTerminal",
            "stopOnEntry": true,
            "justMyCode": false
        }
    ]
}